  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "jest": "^28.1.2",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.4"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
